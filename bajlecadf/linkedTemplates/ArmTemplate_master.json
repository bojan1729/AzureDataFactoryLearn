						"value": "[parameters('lnk_storage_connectionString')]"
					},
					"testcreditsafe_sftp_password": {
						"value": "[parameters('testcreditsafe_sftp_password')]"
					},
					"LNK_DataLakeGen2_properties_typeProperties_url": {
						"value": "[parameters('LNK_DataLakeGen2_properties_typeProperties_url')]"
					},
					"lnk_restapi_example_properties_typeProperties_url": {
						"value": "[parameters('lnk_restapi_example_properties_typeProperties_url')]"
					},
					"lnk_restapi_example_param_properties_typeProperties_url": {
						"value": "[parameters('lnk_restapi_example_param_properties_typeProperties_url')]"
					},
					"testcreditsafe_sftp_properties_typeProperties_host": {
						"value": "[parameters('testcreditsafe_sftp_properties_typeProperties_host')]"
					},
					"testcreditsafe_sftp_properties_typeProperties_userName": {
						"value": "[parameters('testcreditsafe_sftp_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "bajlecadf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
