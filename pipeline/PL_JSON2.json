{
	"name": "PL_JSON2",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "EmployeeId",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								},
								"sink": {
									"name": "EmployeeName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								},
								"sink": {
									"name": "EmployeeSalary",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								},
								"sink": {
									"name": "EmployeeAge",
									"type": "Int32"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "DS_storage_employees",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_sqldb",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "dymap",
							"TableName": "tgtEmployees"
						}
					}
				]
			}
		],
		"folder": {
			"name": "dynamic_mapping"
		},
		"annotations": []
	}
}